void serialEvent() {
  char instr[200];
  int i = 0;
  while (Serial.available()) {
    instr[i++] = (char) Serial.read();
  }

  switch (instr) {
    case "front":
      break;
    case "back":
      break;
    case "off":
      break;
  }
}

